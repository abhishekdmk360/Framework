<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">

<groups>
	<run>
		<include name="ErrorHandling"></include>
	</run>
</groups>

  <test thread-count="5" name="Place Order Test">
    <classes>
      <class name="javaCoding.Testing.PlaceOrderTesting"/>
    </classes>
  </test>
  
  <test name="Error Validations Test">
  	<classes>
  		<class name="javaCoding.Testing.ErrorValidationsTesting"/>
  	</classes>
  </test>
</suite>
